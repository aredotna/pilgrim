.saved-path-link
  position fixed
  right 100px
  top 0px
  width 260px
  height topbar-height
  text-align right
  padding-right unit
  display flex
  justify-content flex-end
  align-items right

  span
    action-link()
    line-height topbar-height

  input[type="text"]
    code()
    background-color transparent
    color light-text
    width 100%
    font-size 10px
    padding 5px
    border none

    &:focus
      outline none
